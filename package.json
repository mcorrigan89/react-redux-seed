    "prettier": "1.7.4",
    "redux-mock-store": "^1.3.0",
    "tslint-config-prettier": "^1.5.0",
    "tslint-plugin-prettier": "^1.3.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dashboard": "^1.0.0",
    "webpack-module-hot-accept": "^1.0.5"
  },
  "lint-staged": {
    "*.{jsx,js,tsx}": [
      "prettier --write",
      "git add"
    ]
  }
