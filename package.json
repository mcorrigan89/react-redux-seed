{
  "name": "react-redux-seed",
  "version": "0.1.0",
  "description": "React Redux Seed",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dashboard -- ./node_modules/.bin/webpack-dev-server -d --config ./config/webpack.config.js",
    "build": "./node_modules/.bin/webpack --config ./config/webpack.config.js",
    "build-analyze": "./node_modules/.bin/webpack --config ./config/webpack.config.js --env.analyze",
    "lint": "./node_modules/.bin/tslint --fix -c ./config/tslint.json -p ./config/tsconfig.json",
    "tslint-check": "tslint-config-prettier-check ./config/tslint.json",
    "prettier": "prettier --write --config ./config/.prettierrc \"src/**/*.{tsx,jsx,js}\"",
    "test": "node_modules/.bin/jest --forceExit --no-cache  --config config/jest.config.js",
    "test-report": "npm test && node_modules/.bin/live-server ./test_reports/lcov-report",
    "precommit": "lint-staged"
  },
  "keywords": [
    "react",
    "redux",
    "seed",
    "typescript"
  ],
  "author": "Michael Corrigan",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^7.1.4",
    "axios": "^0.17.0",
    "babel-loader": "^7.1.2",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "debug": "^3.1.0",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "glamor": "^2.20.40",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "jest-css-modules": "^1.1.0",
    "minimist": "^1.2.0",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.12",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-scss": "^1.0.2",
    "precss": "^2.0.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-dom-factories": "^1.0.2",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-portal": "^3.1.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
