    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.19.0",
    "svg-inline-loader": "^0.8.0",
    "ts-jest": "^21.0.1",
    "ts-loader": "^3.1.0",
    "tslint": "^5.7.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.5.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.43",
    "@types/enzyme": "^3.1.1",
    "@types/jest": "^21.1.1",
    "@types/node": "^8.0.31",
    "@types/react": "^16.0.7",
    "@types/react-dom": "^16.0.1",
    "@types/react-redux": "^5.0.9",
    "@types/react-test-renderer": "^16.0.0",
    "@types/redux": "^3.6.31",
    "@types/redux-mock-store": "0.0.12",
    "@types/webpack-env": "^1.13.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.3",
